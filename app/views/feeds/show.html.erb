<h1 class="feed-title">Welcome <%= current_user.username %>!</h1>
<section class="feed">
  <article class="feed-messages">
    <b>Your Feed!</b>
    <ul id="feed-messages-list">
      <% @feed_tweets.each do |tweet| %>
        <li><%= render 'tweets/tweet', tweet: tweet %></li>
      <% end %>
    </ul>
  </article>
  <aside class="feed-form">
    <%= render 'tweets/form' %>
  </aside>
</section>
